{
  "format": "custom",
  "id": "products",
  "label": "Products",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "name": {
        "type": "Text",
        "config": {
          "label": "Name",
          "placeholder": "Enter the name of the Product"
        }
      },
      "category": {
        "type": "Text",
        "config": {
          "label": "Category",
          "placeholder": "Which Category it is ?"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "description": {
        "type": "Text",
        "config": {
          "label": "Description",
          "placeholder": "Give a short description about the product"
        }
      },
      "highlight": {
        "type": "Text",
        "config": {
          "label": "Highlight",
          "placeholder": "Give a Highlight of this product"
        }
      },
      "specs": {
        "type": "Group",
        "config": {
          "label": "Specs",
          "repeat": true,
          "fields": {
            "spec": {
              "type": "Text",
              "config": {
                "label": "Spec",
                "placeholder": "E.g., 400W - 550W capacity"
              }
            }
          }
        }
      }
    }
  }
}
